    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "sass-lint": "^1.13.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.32.0",
    "webpack-cli": "^3.3.2",
