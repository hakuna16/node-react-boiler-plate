{
  "name": "node-react-boiler-plate",
  "version": "1.0.0",
  "description": "React client - Only the static react Site.",
  "main": "index.js",
  "license": "ISC",
  "author": "Rituj",
  "scripts": {
    "start": "webpack-dev-server --open --config ./configs/webpack.config.dev.js",
    "build:prod": "webpack --config ./configs/webpack.config.prod.js",
    "clean": "rm -rf build/",
    "build": "concurrently \"yarn run clean\" \"yarn run build:prod\"",
    "server": " node server/server.js",
    "test": "jest --color",
    "coverage": "jest --coverage",
    "sass-lint": "sass-lint -vq src/**/*.scss",
    "lint": "node_modules/.bin/eslint \"src/**/*.js\"",
    "lint-fix": "node_modules/.bin/eslint \"src/**/*.js\" --fix",
    "doc": "jsdoc -c jsdoc.conf.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "compression-webpack-plugin": "^2.0.0",
    "concurrently": "^4.1.0",
    "core-js": "^2.5.7",
    "express": "^4.17.0",
    "file-loader": "^3.0.1",
    "history": "^4.9.0",
    "lodash": "^4.17.11",
    "polished": "^3.3.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-form": "^8.2.1",
    "redux-saga": "^1.0.2",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
